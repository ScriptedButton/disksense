{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "fs:deny-seek",
    "fs:allow-stat",
    "fs:allow-size",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-open",
    "shell:default",
    "shell:default",
    "shell:default"
  ]
}