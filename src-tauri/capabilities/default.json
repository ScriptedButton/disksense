{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:read-all",
    "fs:deny-seek",
    "fs:allow-stat",
    "fs:allow-size"
  ]
}
